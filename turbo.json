{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/master",
    "pipeline": {
        "build": {
          "dependsOn": ["^build"],
          "outputs": [".next/**"]
        },
        "test": {
          "dependsOn": ["^build"],
          "outputs": []
        },
        "lint": {
          "outputs": []
        },
        "start": {
          "cache": false
        }
      }
}